function mili(wait)

    local start := seconds()
    
    do while seconds() < start + (wait/1000)
    endDo

return

function zeroum()
return mod(int(val(right(str(seconds()),1))),2)

function sleep(nSeconds)

    // RUN ("sleep "+str(nSeconds))

    local t
    for t = 1 to nSeconds*100
        run sleep 0.01
    end

return